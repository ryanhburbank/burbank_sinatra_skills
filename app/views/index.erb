<div class="container">
  <h1>User Skills and Levels</h1>
    <table>
      <tr>
      <th>Name</th>
      <th>Skills</th>
      <th>Years</th>
      <th>Formal</th>
      </tr>
        <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td>
            <table>
              <% user.proficiencies.each do |proficiency| %>
                <tr><td><%= proficiency.skill.name %></td></tr>
              <% end %>
            </table>
        </td>
        <td>
            <table>
              <% user.proficiencies.each do |proficiency| %>
                <tr><td><%= proficiency.years %></td></tr>
              <% end %>
            </table>
        </td>
         <td>
            <table>
              <% user.proficiencies.each do |proficiency| %>
                <tr><td><%= proficiency.formal == 1 ? "Yes" : "No" %></td></tr>
              <% end %>
            </table>
        </td>
      </tr>
      <% end %>
    </table>
